<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2005 sp2 U (http://www.altova.com) by Vogeler, Georg (Ludwigs-Maximilians-Universität) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="cei">
		<xs:annotation>
			<xs:documentation xml:lang="de">Wurzelelement für alle Dokumente, die den Regeln der 
			CEI (Charters Encoding Initiative, http://www.cei.lmu.de) folgen.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="teiHeader">
					<xs:annotation>
						<xs:documentation xml:lang="de">Der Dateikopf beschreibt die Datei nach den Regeln der TEI (http://www.tei-c.org), Version P4</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="fileDesc">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="titleStmt">
											<xs:annotation>
												<xs:documentation xml:lang="de">Den Regeln der TEI entsprechend müßten hier verschiedene Elemente verpflichtend sein: title z.B. [strict!]</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:choice minOccurs="0" maxOccurs="unbounded">
														<xs:element ref="title"/>
														<xs:element name="author"/>
														<xs:element ref="p"/>
													</xs:choice>
												</xs:sequence>
												<xs:attributeGroup ref="standard"/>
											</xs:complexType>
										</xs:element>
										<xs:element ref="publicationStmt" minOccurs="0"/>
										<xs:element ref="sourceDesc" minOccurs="0"/>
									</xs:sequence>
									<xs:attributeGroup ref="standard"/>
								</xs:complexType>
							</xs:element>
							<xs:element ref="encodingDesc" minOccurs="0"/>
							<xs:element name="projectDesc" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:choice minOccurs="0" maxOccurs="unbounded">
											<xs:element ref="p"/>
											<xs:element ref="respStmt"/>
										</xs:choice>
									</xs:sequence>
									<xs:attributeGroup ref="standard"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attributeGroup ref="standard"/>
					</xs:complexType>
				</xs:element>
				<xs:element ref="text"/>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
			<xs:attributeGroup ref="transcription"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="encodingDesc">
		<xs:complexType mixed="true">
			<xs:complexContent mixed="true">
				<xs:extension base="xs:anyType">
					<xs:attributeGroup ref="standard"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="sourceDesc">
		<xs:annotation>
			<xs:documentation xml:lang="de">enthält die bibliographischen Angaben der Quellen, aus denen der elektronische Text stammt.</xs:documentation>
			<xs:documentation xml:lang="en">supplies a bibliographic description of the copy text(s) from which an electronic text was derived or generated.</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice minOccurs="0">
					<xs:group ref="biblArch"/>
					<xs:element ref="p" maxOccurs="unbounded"/>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
			<xs:attribute name="ref">
				<xs:annotation>
					<xs:documentation xml:lang="de">Verweis auf die Textpassage zu der hier die Quelle angegeben wird.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="publicationStmt">
		<xs:annotation>
			<xs:documentation xml:lang="de">Angaben zur Veröffentlichung des Textes (Zeitpunkt, rechtsverbindlicher Ort, Beschränkungen, Verantwortlichkeiten ...)</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element name="publisher">
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="xs:anySimpleType">
									<xs:attributeGroup ref="standard"/>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="pubPlace">
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="xs:anySimpleType">
									<xs:attributeGroup ref="standard"/>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="availability">
						<xs:complexType mixed="true">
							<xs:attributeGroup ref="standard"/>
							<xs:attribute name="status" type="xs:anySimpleType"/>
						</xs:complexType>
					</xs:element>
					<xs:group ref="date"/>
					<xs:element ref="p"/>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="respStmt">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="name"/>
					<xs:element name="resp">
						<xs:annotation>
							<xs:documentation xml:lang="en">contains a phrase describing the nature of a person's intellectual responsibility.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="xs:string">
									<xs:attributeGroup ref="standard"/>
									<xs:attributeGroup ref="name"/>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:group ref="date"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="text">
		<xs:annotation>
			<xs:documentation xml:lang="de">Mit type="charter" als Urkunde identifiziert</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice>
				<xs:sequence minOccurs="0">
					<xs:element name="front" minOccurs="0">
						<xs:complexType mixed="true">
							<xs:sequence>
								<xs:choice minOccurs="0" maxOccurs="unbounded">
									<xs:element name="head" type="xs:string"/>
									<xs:group ref="biblArch"/>
									<xs:element ref="sourceDesc"/>
									<xs:element ref="publicationStmt"/>
									<xs:element ref="respStmt"/>
									<xs:group ref="textualElements"/>
									<xs:group ref="reference"/>
									<xs:element ref="encodingDesc"/>
								</xs:choice>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:choice>
						<xs:element name="body">
							<xs:complexType>
								<xs:choice>
									<xs:sequence minOccurs="0" maxOccurs="unbounded">
										<xs:choice minOccurs="0" maxOccurs="unbounded">
											<xs:element ref="p"/>
										</xs:choice>
									</xs:sequence>
									<xs:sequence minOccurs="0">
										<xs:element ref="idno" minOccurs="0"/>
										<xs:element ref="chDesc" minOccurs="0"/>
										<xs:element ref="tenor" minOccurs="0"/>
									</xs:sequence>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="group" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:choice maxOccurs="unbounded">
										<xs:element ref="h1"/>
										<xs:element ref="text"/>
									</xs:choice>
								</xs:sequence>
								<xs:attributeGroup ref="standard"/>
							</xs:complexType>
						</xs:element>
					</xs:choice>
					<xs:element name="back" type="xs:anyType" minOccurs="0"/>
				</xs:sequence>
				<xs:sequence minOccurs="0">
					<xs:element name="idno" minOccurs="0"/>
					<xs:element ref="chDesc" minOccurs="0"/>
					<xs:element ref="tenor" minOccurs="0"/>
				</xs:sequence>
			</xs:choice>
			<xs:attributeGroup ref="standard"/>
			<xs:attribute name="type"/>
			<xs:attribute ref="picture"/>
			<xs:attribute ref="certainty"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="idno" type="xs:anySimpleType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Die Identifikationsnummer eines Objektes innerhalb der aktuellen Struktur (z.B. Nummer in einem Urkundenbuch, Bestand in einem Archiv, Handschriftensignatur in einer Bibliothek. Ist gegenüber 'refNum' vorzuziehen. [strict!]</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="chDesc">
		<xs:annotation>
			<xs:documentation xml:lang="de">Faßt die Metadaten einer Urkunde zusammen.</xs:documentation>
			<xs:documentation xml:lang="en">contains the meta-data for the charter</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element name="abstract">
						<xs:annotation>
							<xs:documentation xml:lang="de">Das Regest, das den Rechtsinhalt einer Urkunde zusammenfaßt, in Editionen als kurzes "Kopfregest", in anderen Umgebungen auch als ausführliches "Langregest" mit wörtlichen Zitaten. Das Regest sollte den Aussteller und den Empfänger der Urkunde nennen. [Fließtext]</xs:documentation>
						</xs:annotation>
						<xs:complexType mixed="true">
							<xs:sequence>
								<xs:choice minOccurs="0" maxOccurs="unbounded">
									<xs:group ref="Sachauszeichnung"/>
									<xs:element ref="quote"/>
									<xs:element ref="foreign"/>
									<xs:element ref="num"/>
									<xs:element ref="issuer"/>
									<xs:element ref="recipient"/>
								</xs:choice>
							</xs:sequence>
							<xs:attributeGroup ref="standard"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="issued">
						<xs:annotation>
							<xs:documentation xml:lang="de">Diese Konstruktion kann allgemeine Elemente für die Beschreibung von Ausstellungsort und -zeit verwenden und ist deshalb den Konstrukten "issuePlace" und "date" bzw. "publicationStmt" vorzuziehen.	[strict!]</xs:documentation>
						</xs:annotation>
						<xs:complexType mixed="true">
							<xs:sequence>
								<xs:choice minOccurs="0" maxOccurs="unbounded">
									<xs:group ref="date"/>
									<xs:choice>
										<xs:element ref="issuePlace"/>
										<xs:element ref="placeName"/>
									</xs:choice>
								</xs:choice>
							</xs:sequence>
							<xs:attributeGroup ref="standard"/>
						</xs:complexType>
					</xs:element>
					<xs:element ref="witList"/>
					<xs:element ref="diplomaticAnalysis"/>
					<xs:element name="class" type="xs:anySimpleType">
						<xs:annotation>
							<xs:documentation xml:lang="en">classifciation of the document</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="issuePlace">
		<xs:annotation>
			<xs:documentation xml:lang="de">Ausstellungsort, zu ersetzen durch publStmt/publPlace? [strict!]</xs:documentation>
			<xs:documentation xml:lang="en">place, where the charter is issued</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="textualElements"/>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="figure">
		<xs:annotation>
			<xs:documentation xml:lang="de">Referenz auf ein Bild</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="figDesc" minOccurs="0">
					<xs:complexType mixed="true">
						<xs:sequence>
							<xs:choice minOccurs="0" maxOccurs="unbounded">
								<xs:group ref="layout"/>
								<xs:group ref="biblArch"/>
							</xs:choice>
						</xs:sequence>
						<xs:attributeGroup ref="standard"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="graphic" minOccurs="0">
					<xs:complexType mixed="true">
						<xs:attribute name="url"/>
						<xs:attributeGroup ref="standard"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="witList">
		<xs:annotation>
			<xs:documentation xml:lang="de">Textzeugen</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element name="witness" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Der einzelne Textzeuge [Liste]</xs:documentation>
						</xs:annotation>
						<xs:complexType mixed="true">
							<xs:sequence>
								<xs:choice minOccurs="0" maxOccurs="unbounded">
									<xs:element name="traditioForm">
										<xs:annotation>
											<xs:documentation xml:lang="de">Überlieferungsform: Original, Kopial, Insert, Transsumpt, Vidimus, Konzept. Die verwendete Begrifflichkeit ist dem VID zu entnehmen oder in der profileDesc zu dokumentieren.</xs:documentation>
										</xs:annotation>
										<xs:complexType mixed="true">
											<xs:sequence>
												<xs:choice minOccurs="0" maxOccurs="unbounded">
													<xs:group ref="reference"/>
													<xs:group ref="layout"/>
													<xs:group ref="textualElements"/>
												</xs:choice>
											</xs:sequence>
											<xs:attributeGroup ref="standard"/>
										</xs:complexType>
									</xs:element>
									<xs:element name="sigil">
										<xs:annotation>
											<xs:documentation xml:lang="de">TEI-P5 sieht dafür das id-Attribut vor, was Sinn macht. Einziges Gegenargument, das mir einfällt: Eine ID ist eigentlich dokumentweit eindeutig, eine Sigle ist in Urkundeneditionen nur innerhalb der einen Urkunde eindeutig. Element vermeiden? [strict!]</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="xs:string">
													<xs:attributeGroup ref="standard"/>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="physicalDesc">
										<xs:complexType mixed="true">
											<xs:sequence>
												<xs:choice minOccurs="0" maxOccurs="unbounded">
													<xs:element ref="scribe"/>
													<xs:group ref="layout"/>
													<xs:group ref="textualElements"/>
													<xs:element name="material">
														<xs:annotation>
															<xs:documentation xml:lang="de">Beschreibstoff</xs:documentation>
															<xs:documentation xml:lang="en">describes the material on which the document is written</xs:documentation>
														</xs:annotation>
														<xs:complexType mixed="true"/>
													</xs:element>
													<xs:element ref="dimensions"/>
													<xs:element ref="sealDesc"/>
													<xs:element name="condition">
														<xs:complexType>
															<xs:sequence>
																<xs:choice minOccurs="0" maxOccurs="unbounded">
																	<xs:group ref="reference"/>
																	<xs:group ref="layout"/>
																	<xs:group ref="textualElements"/>
																</xs:choice>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="extent">
														<xs:complexType mixed="true">
															<xs:sequence>
																<xs:choice minOccurs="0" maxOccurs="unbounded">
																	<xs:element ref="dimensions"/>
																</xs:choice>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:choice>
											</xs:sequence>
											<xs:attributeGroup ref="standard"/>
										</xs:complexType>
									</xs:element>
									<xs:element ref="nota"/>
									<xs:element name="auth">
										<xs:annotation>
											<xs:documentation xml:lang="en">describes the authentification of the document.</xs:documentation>
										</xs:annotation>
										<xs:complexType mixed="true">
											<xs:sequence>
												<xs:choice minOccurs="0" maxOccurs="unbounded">
													<xs:element ref="sealDesc"/>
													<xs:group ref="layout"/>
													<xs:element ref="subscriptio"/>
												</xs:choice>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:group ref="Sachauszeichnung"/>
									<xs:group ref="layout"/>
									<xs:group ref="textualElements"/>
									<xs:group ref="reference"/>
									<xs:group ref="biblArch"/>
								</xs:choice>
							</xs:sequence>
							<xs:attribute ref="certainty" use="optional"/>
							<xs:attribute name="type" type="xs:anySimpleType" use="optional"/>
							<xs:attributeGroup ref="standard"/>
							<xs:attribute name="sigil" type="xs:anySimpleType">
								<xs:annotation>
									<xs:documentation xml:lang="de">P5 sieht dafür das Attribut id vor. Diese Lösung ist sowohl dem Element, als auch dem Attribut vorzuziehen. [strict!]</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:group ref="layout"/>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="sealDesc">
		<xs:annotation>
			<xs:documentation xml:lang="de">eine differenzierte Siegelbeschreibung [Fließtext]</xs:documentation>
			<xs:documentation xml:lang="de">[Liste]</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="sigillant"/>
					<xs:group ref="Sachauszeichnung"/>
					<xs:element name="legend">
						<xs:complexType mixed="true">
							<xs:sequence>
								<xs:group ref="transcription"/>
							</xs:sequence>
							<xs:attributeGroup ref="standard"/>
							<xs:attribute name="place"/>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="listBibl">
		<xs:annotation>
			<xs:documentation xml:lang="de">Bibliographische Auflistungen.</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="bibl"/>
					<xs:element ref="scope"/>
					<xs:element ref="ref" minOccurs="0"/>
					<xs:element ref="biblStruct"/>
					<xs:group ref="layout"/>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
			<xs:attribute ref="type">
				<xs:annotation>
					<xs:documentation xml:lang="de">Im Rahmen der diplomatischen Analyse sind insbesondere folgende Typen vorgesehen: "print" (Editionen), "regesta" (Regestenwerke), "facsimilia" (Reproduktionen), "studies" (Literatur, die sich mit der Urkunde beschäftigt).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="tenor">
		<xs:annotation>
			<xs:documentation xml:lang="de">Der eigentliche Urkundentext. [Fließtext]</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="Originalbefund"/>
					<xs:group ref="Sachliches"/>
					<xs:group ref="Formular"/>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="note">
		<xs:annotation>
			<xs:documentation xml:lang="de">Ein ergänzender Text zu einer bestimmten Textstelle, der im Druck gewöhnlich als Marginalie, Fuß- oder Endnote dargestellt wird. [Fließtext]</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="Originalbefund"/>
					<xs:group ref="Sachliches"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="place" type="xs:anySimpleType" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="de">Gibt die auch auf ihrer Funktion beruhende Position der Fußnote zum Text an: foot/end/margin; content|textcrit</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="standard"/>
			<xs:attributeGroup ref="reference"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="p">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="formularium"/>
					<xs:group ref="layout"/>
					<xs:group ref="reference"/>
					<xs:group ref="Sachauszeichnung"/>
					<xs:group ref="transcription"/>
					<xs:group ref="biblArch"/>
					<xs:group ref="textualElements"/>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
		</xs:complexType>
	</xs:element>
	<xs:group name="relevantPersonal">
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="testis"/>
				<xs:element ref="scribe"/>
				<xs:element ref="recipient"/>
				<xs:element ref="issuer"/>
				<xs:element ref="sigillant"/>
				<xs:element name="relevantPersonal">
					<xs:complexType mixed="true">
						<xs:group ref="layout"/>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:group>
	<xs:element name="scribe">
		<xs:annotation>
			<xs:documentation>eine Textpassage, die einen Schreiber beschreibt. [strict!] Wie macht das die TEI mit handList und hand? Evtl. ist die Hand nur eine Teilfunktion des Schreibers, der ja auch anderweitig als Person beschrieben werden kann?</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="layout"/>
					<xs:group ref="textualElements"/>
					<xs:group ref="reference"/>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="standard"/>
			<xs:attributeGroup ref="name"/>
			<xs:attribute ref="hand"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="sigillant">
		<xs:complexType mixed="true">
			<xs:group ref="layout"/>
