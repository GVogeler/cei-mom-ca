			</xs:sequence>
			<xs:sequence>
				<xs:element ref="invocatio" minOccurs="0" maxOccurs="unbounded"/>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="intitulatio" minOccurs="0"/>
					<xs:element ref="inscriptio" minOccurs="0"/>
				</xs:choice>
				<xs:element ref="publicatio" minOccurs="0"/>
				<xs:element ref="arenga" minOccurs="0"/>
				<xs:element ref="narratio" minOccurs="0"/>
				<xs:element ref="dispositio" minOccurs="0"/>
				<xs:element ref="corroboratio" minOccurs="0"/>
				<xs:element ref="sanctio" minOccurs="0"/>
				<xs:element ref="subscriptio" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="datatio" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="apprecatio" minOccurs="0"/>
			</xs:sequence>
			<xs:element ref="setPhrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:group>
	<xs:element name="invocatio">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
