						<xs:sequence>
							<xs:choice minOccurs="0" maxOccurs="unbounded">
								<xs:element ref="lb" minOccurs="0"/>
								<xs:element ref="pb" minOccurs="0"/>
								<xs:element ref="p" minOccurs="0"/>
								<xs:group ref="transcription"/>
								<xs:group ref="Sachauszeichnung"/>
								<xs:group ref="reference"/>
								<xs:element ref="invocatio"/>
								<xs:element ref="intitulatio"/>
								<xs:element ref="inscriptio"/>
								<xs:element ref="publicatio"/>
								<xs:element ref="sup"/>
